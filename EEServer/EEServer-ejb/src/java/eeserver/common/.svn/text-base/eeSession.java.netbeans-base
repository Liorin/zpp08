package eeserver.common;

import eeserver.persistence.eeUser;

/**
 *
 * @author maciek
 */
public class eeSession {

    protected Integer sessionID;

    /**
     * Get the value of sessionID
     *
     * @return the value of sessionID
     */
    public Integer getSessionID() {
        return sessionID;
    }

    /**
     * Set the value of sessionID
     *
     * @param sessionID new value of sessionID
     */
    public void setSessionID(Integer sessionID) {
        this.sessionID = sessionID;
    }


    protected eeUser user;

    /**
     * Get the value of user
     *
     * @return the value of user
     */
    public eeUser getUser() {
        return user;
    }

    /**
     * Set the value of user
     *
     * @param user new value of user
     */
    public void setUser(eeUser user) {
        this.user = user;
    }

    public eeSession(eeUser user, Integer sessionID) {
        this.user = user;
        this.sessionID = sessionID;
    }


}
